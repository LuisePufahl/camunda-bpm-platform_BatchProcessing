      <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:sequenceFlow id="SequenceFlow_1" name="" sourceRef="StartEvent_1" targetRef="UT_ApproveOrder" />
    <bpmn2:exclusiveGateway id="ExclusiveGateway_1">
